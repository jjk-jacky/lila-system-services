#!@BINDIR@/execlineb -P
aa-service
elglob -s dirs /etc/tweaks/${INSTANCE}/*
forx -p dir { ${dirs} } importas -u dir dir
foreground {
  backtick -n name { pipeline { echo ${dir} } sed s/.\\+\\/// }
  importas -u name name echo * Applying ${name} ...
}
backtick -in file { cat ${dir}/file }
importas -u file file
redirfd -w 1 ${file}
cat ${dir}/value
